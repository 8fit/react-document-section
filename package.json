{
  "name": "react-document-section",
  "version": "1.0.1",
  "description": "Modular creation of the document outline by automating heading level assignment with React components",
  "author": "8fit",
  "license": "MIT",
  "repository": "8fit/react-document-section",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 jest",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "typegen": "tsc -p ./tsconfig.json --emitDeclarationOnly",
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c",
    "postbuild": "npm run typegen",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {},
  "peerDependencies": {
    "prop-types": ">=15.7.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.12",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-jest": "^24.8.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.17.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.3.2",
    "typescript": "^3.4.5"
  },
  "files": [
    "dist"
  ]
}
